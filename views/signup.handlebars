<div class="row justify-space-between-md">
  <div class="signup-card text-center">
    <h2 class="page-title">Sign Up</h2>
    
    <form class="form signup-form" id="signup-form">
  
      <div class="form-group">
        <label for="username-signup">Username:</label>
        <input class="form-input" type="text" id="username-signup" />
      </div>
  
      <div class="form-group">
        <label for="password-signup">Password:</label>
        <input class="form-input" type="password" id="password-signup" />
      </div>
  
      <div class="form-group">
        <button class="btn btn-primary" type="submit" id="submit-button">SIGN UP</button>
      </div>
  
    </form>
  
  </div>
</div>

<script src="./js/signup.js"></script>